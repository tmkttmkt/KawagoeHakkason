{
    "version": 2,
    "projects": {
      "rootDirectory": "frontend"
    },
    "builds": [
      {
        "src": "src/index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  